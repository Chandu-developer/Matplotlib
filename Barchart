import matplotlib.pyplot as plt

x1 = [2,4,6,8]
y1 = [3,5,7,3]

x2 = [1,3,5,7,9]
y2 = [2,5,4,9,7]

plt.bar(x1,y1,label = "Bar1",color = "red")
plt.bar(x2,y2,label = "Bar2", color = "Blue")

plt.xlabel("X-axis")
plt.ylabel("Y-axis")
plt.title("BAR chart")

plt.legend() #This shows which color is assigned to which label

plt.show()
